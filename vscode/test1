List=[[0 for _ in range(19)] for _ in range(19)]

n=int(input())

for _ in range(n):
    
    x,y=map(int,input().split())
    
    for i in range(19):
    
        for j in range(19):
            if i==(x-1) and j==(y-1) :
                List[i][j]+=1
                if List[i][j]!=1:
                    List[i][j]==1

                

for i in range(19):
    for j in range(19):
        print(List[i][j],end=" ")
    print()




